lshift - down : yabai -m window --focus south
lshift - up : yabai -m window --focus north
lshift - left : yabai -m window --focus west
lshift - right : yabai -m window --focus east

ctrl - r : yabai -m space --rotate 270
ctrl - x : yabai -m space --mirror y-axis
ctrl - space : yabai -m window --toggle float --grid 4:4:1:1:2:2

ctrl - f : yabai -m window --toggle zoom-fullscreen
ctrl - i : yabai -m space --balance

ctrl - left : yabai -m window --warp west
ctrl - up : yabai -m window --warp north
ctrl - right : yabai -m window --warp east 
ctrl - down : yabai -m window --warp south

ctrl + cmd - 1 : yabai -m window --space 1
ctrl + cmd - 2 : yabai -m window --space 2
ctrl + cmd - 3 : yabai -m window --space 3
ctrl + cmd - 4 : yabai -m window --space 4
ctrl + cmd - 5 : yabai -m window --space 5
ctrl + cmd - 6 : yabai -m window --space 6
ctrl + cmd - 7 : yabai -m window --space 7

ctrl - d : yabai -m window --resize right:100:0; yabai -m window --resize left:100:0
ctrl - a : yabai -m window --resize right:-100:0; yabai -m window --resize left:-100:0
ctrl - w: yabai -m window --resize top:0:-100; yabai -m window --resize bottom:0:-100; 
ctrl - s: yabai -m window --resize top:0:100; yabai -m window --resize bottom:0:100










