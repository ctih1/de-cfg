lshift - down : yabai -m window --focus south
lshift - up : yabai -m window --focus north
lshift - left : yabai -m window --focus west
lshift - right : yabai -m window --focus east

cmd - r : yabai -m space --rotate 270
cmd + shift - r : yabai -m space --mirror y-axis
shift + cmd - t : yabai -m window --toggle float --grid 4:4:1:1:2:2

shift + cmd - f : yabai -m window --toggle zoom-fullscreen
shift + cmd - g : yabai -m space --balance

shift + cmd - left : yabai -m window --warp west
shift + cmd - up : yabai -m window --warp north
shift + cmd - right : yabai -m window --warp east 
shift + cmd - down : yabai -m window --warp south

shift + ctrl - 1 : yabai -m window --space 1
shift + ctrl - 2 : yabai -m window --space 2
shift + ctrl - 3 : yabai -m window --space 3
shift + ctrl - 4 : yabai -m window --space 4
shift + ctrl - 6 : yabai -m window --space 6
shift + ctrl - 5 : yabai -m window --space 5
shift + ctrl - 7 : yabai -m window --space 7
shift + ctrl - 8 : yabai -m window --space 8
shift + ctrl - 9 : yabai -m window --space 9

cmd - right : yabai -m window --resize right:100:0; yabai -m window --resize left:100:0
cmd - left : yabai -m window --resize right:-100:0; yabai -m window --resize left:-100:0
cmd - up: yabai -m window --resize top:0:-100; yabai -m window --resize bottom:0:-100; 
cmd - down: yabai -m window --resize top:0:100; yabai -m window --resize bottom:0:100










